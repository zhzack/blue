<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.blue.mapper.ChildDeviceMapper">


    <select id="query_cdBydId" parameterType="int" resultType="xyz.blue.pojo.ChildDevice">
        select *
        from child_device
        where device_id = #{deviceId}
    </select>

</mapper>